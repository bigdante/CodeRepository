cd ~/.vscode-server/bin
wget https://update.code.visualstudio.com/commit:1a5daa3a0231a0fbba4f14db7ec463cf99d7768e/server-linux-x64/stable

tar -zxvf stable
cp -r vscode-server-linux-x64/* 1a5daa3a0231a0fbba4f14db7ec463cf99d7768e/

apt-get update -y
apt-get install tmux -y
apt-get install wget -y
apt-get install curl -y


No such file or directory: '/usr/local/cuda/bin/nvcc'
conda install -c nvidia cuda-nvcc



apt-get update -y
apt-get install tmux -y

__conda_setup="$('/workspace/xll/Anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/workspace/xll/Anaconda3/etc/profile.d/conda.sh" ]; then
        . "/workspace/xll/Anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/workspace/xll/Anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup

conda activate auto
cd /workspace/xll/